{
    "compilerOptions": {
      "target": "es5",
      "module": "esnext",
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "paths": {
        "*": ["./node_modules/*"]
      }
    },
    "include": ["src"]
  }